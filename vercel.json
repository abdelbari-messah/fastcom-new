{
  "rewrites": [
    {
      "source": "/",
      "destination": "/FastCom/home-1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/FastCom/$1"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ]
}
